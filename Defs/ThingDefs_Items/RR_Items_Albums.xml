<?xml version="1.0" ?>
<Defs>
  <ThingDef ParentName="ResourceBase">
    <defName>RR_BlankMedium</defName>
    <label>blank recording medium</label>
    <thingClass>ThingWithComps</thingClass>
    <graphicData>
      <texPath>Things/Items/Albums/blank_medium</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <drawSize>0.8</drawSize>
    </graphicData>
    <tradeability>Sellable</tradeability>
    <stackLimit>1</stackLimit>
    <statBases>
      <Mass>0.05</Mass>
      <MarketValue>6</MarketValue>
      <MaxHitPoints>40</MaxHitPoints>
      <Flammability>0.3</Flammability>
    </statBases>
    <thingCategories>
      <li>Manufactured</li>
    </thingCategories>
    <costList>
      <Steel>5</Steel>
      <ComponentIndustrial>1</ComponentIndustrial>
    </costList>
    <recipeMaker>
      <workAmount>600</workAmount>
      <workSpeedStat>GeneralLaborSpeed</workSpeedStat>
      <workSkill>Crafting</workSkill>
      <skillRequirements>
        <Crafting>2</Crafting>
      </skillRequirements>
      <recipeUsers>
        <li>TableMachining</li>
      </recipeUsers>
    </recipeMaker>
  </ThingDef>
  
  <ThingDef ParentName="ResourceBase">
    <defName>RR_Album_Jazz</defName>
    <label>Jazz album</label>
    <description>A recorded jazz album. It carries quality, an author credit, a title, a genre, and a descriptive blurb tuned to jazz.</description>
    <thingClass>ThingWithComps</thingClass>
    <graphicData>
      <texPath>Things/Items/Albums/album_jazz</texPath>
